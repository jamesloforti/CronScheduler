{
  "Logging": {
    "LogLevel": {
      "Default": "Information",
      "Microsoft.AspNetCore": "Warning"
    },
    "Filters": {
      "Microsoft": "Error",
      "System": "Error"
    }
  },
  "AllowedHosts": "*",
  "AppSettings": {
    "ApplicationName": "WebServiceDefault",
    "Environment": "development",
    "HttpClientTimeoutMinutes": 2,
    "PollyMaxRetries": 3,
    "SwaggerJsonUrl": "/swagger/v1/swagger.json",
    "DownstreamServiceUrl":  "http://www.google.com"
  },
  "AuthSettings": {
    "AuthUrl": "string",
    "ClientId": "string",
    "ClientSecret": "string",
    "HttpClientTimeoutMinutes": 2,
    "PollyMaxRetries": 3,
    "Password": "string",
    "SecurityToken": "string",
    "Username": "string"
  },
  "CronSettings": {
    "TargetMachine": "ServerName",
    "ProcessRecords":  "5 5 * * 1"
  }
}
